<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="practice.itemService.usingJsp.address.AddressMapper">
    <insert id="insert_addr" parameterType="Map">
        INSERT INTO 공통_도로명주소n
        (
            구역번호
        ,시도
        ,시군구
        ,읍면
        ,도로명코드
        ,도로명
        ,지하여부
        ,건물번호본번
        ,건물번호부번
        ,건물관리번호
        ,다량배달처명
        ,시군구용건물명
        ,법정동코드
        ,법정동명
        ,리
        ,행정동명
        ,산여부
        ,지번본번
        ,읍면동일련번호
        ,지번부번
        ,우편번호
        ,우편일련번호
        ,공동주택여부
        ,도로명주소
        ,지번주소
        ,검색용_도로명주소
        ,검색용_지번주소
        ,최종변경일시
        )
        VALUES
            (
                #{구역번호}
            ,#{시도}
            ,#{시군구}
            ,#{읍면}
            ,#{도로명코드}
            ,#{도로명}
            ,#{지하여부}
            ,#{건물번호본번}
            ,#{건물번호부번}
            ,#{건물관리번호}
            ,#{다량배달처명}
            ,#{시군구용건물명}
            ,#{법정동코드}
            ,#{법정동명}
            ,#{리}
            ,#{행정동명}
            ,#{산여부}
            ,#{지번본번}
            ,#{읍면동일련번호}
            ,#{지번부번}
            ,#{우편번호}
            ,#{우편일련번호}
            ,#{공동주택여부}
            ,#{도로명주소}
            ,#{지번주소}
            ,#{검색용_도로명주소}
            ,#{검색용_지번주소}
            ,now()
            )
    </insert>

    <update id="update_addr" parameterType="Map">
        UPDATE 공통_도로명주소n
        SET
            구역번호 = #{구역번호}
          ,시도 = #{시도}
          ,시군구 = #{시군구}
          ,읍면 = #{읍면}
          ,도로명코드 = #{도로명코드}
          ,도로명 = #{도로명}
          ,지하여부 = #{지하여부}
          ,시군구용건물명 = #{시군구용건물명}
          ,법정동코드 = #{법정동코드}
          ,법정동명 = #{법정동명}
          ,리 = #{리}
          ,행정동명 = #{행정동명}
          ,산여부 = #{산여부}
          ,지번본번 = #{지번본번}
          ,읍면동일련번호 = #{읍면동일련번호}
          ,지번부번 = #{지번부번}
          ,우편번호 = #{우편번호}
          ,공동주택여부 = '1'
          ,도로명주소 = #{도로명주소}
          ,지번주소 = #{지번주소}
          ,검색용_도로명주소 = #{검색용_도로명주소}
          ,검색용_지번주소 = #{검색용_지번주소}
          ,최종변경일시 = now()
        WHERE 건물관리번호 = #{건물관리번호}
    </update>

    <delete id="delete_addr" parameterType="Map">
        delete	from	공통_도로명주소n
        WHERE 건물관리번호 = #{건물관리번호}
    </delete>

    <insert id="insert_bubJung" parameterType="Map">
        INSERT INTO 공통_법정동관리m
        (
            시도
        ,시군구
        ,법정동명
        ,법정동코드
        ,리
        ,최종변경일시
        )
        VALUES
            (
                #{시도}
            ,#{시군구}
            ,#{법정동명}
            ,#{법정동코드}
            ,#{리}
            ,now()
            )
    </insert>

    <update id="update_bubJung" parameterType="Map">
        UPDATE 공통_법정동관리m
        SET
            시도 = #{시도}
          ,시군구 = #{시군구}
          ,법정동명 = #{법정동명}
          ,법정동코드 = #{법정동코드}
          ,리 = #{리}
          ,최종변경일시 = now()
        WHERE 법정동코드 = #{법정동코드}
    </update>

    <delete id="delete_bubJung" parameterType="Map">
        delete	from	공통_법정동관리m
        WHERE 법정동코드 = #{법정동코드}
    </delete>


</mapper>